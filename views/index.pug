extends layout

block content
  h1= title
  p Welcome to #{title}
  if user
    p Hello, #{user.username}
    a(href="/logout") Logout
  else
    a(href="/login") Login
  div#block.block
  button#scaling-button 拡大縮小
  button#moving-button1 左右移動
  button#moving-button2 上下移動
  script(src="/javascripts/bundle.js")