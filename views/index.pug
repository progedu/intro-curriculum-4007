extends layout

block content
  h1= title
  p Welcome to #{title}
  if user
    p Hello, #{user.username}
    a(href="/logout") Logout
  else
    a(href="/login") Login
  div#block.block
  button#scaling-button 拡大縮小
  br
  button#moving-button 移動
  br
  button#change-button 変形
  script(src="/javascripts/bundle.js")