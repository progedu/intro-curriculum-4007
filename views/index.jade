extends layout

block content
  h1= title
  p Welcome to #{title}
  if user
    p Hello, #{user.username}
    a(href="/logout") Logout
  else
    a(href="/login") Login
  div(id="block", class="block")
  a(margin)
  button(id="scaling-button") 拡大縮小
  button(style="margin-left:20px", id="moving-button") 移動
  button(style="margin-left:20px", id="color-button") 色変更
  script(src="/javascripts/bundle.js")
